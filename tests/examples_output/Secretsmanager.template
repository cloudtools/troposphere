{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "MySecret": {
            "Properties": {
                "Description": "This is an autogenerated secret",
                "GenerateSecretString": {
                    "GenerateStringKey": "password",
                    "PasswordLength": 30,
                    "SecretStringTemplate": "{\"username\":\"test-user\"}"
                },
                "Name": "MySecret",
                "Tags": [
                    {
                        "Key": "Appname",
                        "Value": "AppA"
                    }
                ]
            },
            "Type": "AWS::SecretsManager::Secret"
        }
    }
}
